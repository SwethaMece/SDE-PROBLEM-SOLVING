#include <iostream>
using namespace std;
int main() {
    int n=3;
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<=i;j++)
        {
            cout<<" * ";
        }
        cout<<endl;
    }

    return 0;
}

//* 
//* * 
//* * *pattern printing using loop first loop indicates the rows and second loop indicates the coloums 
